# DevNet Configuration Example
# Copy this file to .env.devnet and adjust values as needed

# Network Configuration
CHAIN_ID="localnet-1"
NODE="http://localhost:26657"

# Wallet Configuration
KEYRING_BACKEND="test"
WALLET_NAME="devnet-deployer"

# Gas Configuration
GAS_PRICES="0.025uatom"
GAS="auto"
GAS_ADJUSTMENT="1.3"

# Contract Configuration (optional - will use defaults if not set)
# VALIDATOR="cosmosvaloper1..." # Leave empty to auto-detect
# MAX_CAP="1000000000000" # Leave empty for unlimited
STAKING_DENOM="uatom"
